#include<bits/stdc++.h>
#define ll  long long
using namespace std;
int main(){
    int tc; cin>>tc;
    int c=1;
    while(tc--){
        int n;
        cin>>n;
        int a[n+3];
        int i;
        for(i=0; i<n; i++)  cin>>a[i];
        sort(a, a+n);
        cout<<"Case "<<c++<<": "<<(a[n-1]-a[0])<<"\n";
    }
    return 0;
}
